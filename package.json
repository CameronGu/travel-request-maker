    "@supabase/supabase-js": "^2.50.4",
    "@tanstack/react-query": "^5.82.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.6.1",
    "libphonenumber-js": "^1.12.9",
    "lucide-react": "^0.510.0",
    "next": "^15.3.5",
    "next-themes": "^0.4.6",
    "pako": "^2.1.0",
