{
  "name": "travel-request-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "dotenv -e .env.local -- vitest",
    "tasks": "task-master list",
    "test:rls:seed": "dotenv -e .env.local -- node src/scripts/seed-rls-test-data.js",
    "test:rls": "pnpm run test:rls:seed && dotenv -e .env.local -- node src/scripts/rls-test.js",
    "ci:test:rls": "pnpm run test:rls && node -e \"process.exit(require('.taskmaster/test-results/rls-test-latest.json').failed ? 1 : 0)\"",
    "test:middleware": "dotenv -e .env.local -- node src/scripts/test-middleware.js",
    "enforce-pnpm": "if [ \"$PNPM_PACKAGE_MANAGER\" = \"true\" ] || [ -n \"$npm_config_user_agent\" ] && echo $npm_config_user_agent | grep -q 'pnpm'; then exit 0; else echo '‚ùå Please use pnpm for installing dependencies. (Detected non-pnpm install)'; exit 1; fi",
    "preinstall": "pnpm run enforce-pnpm"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-slot": "^1.2.3",
